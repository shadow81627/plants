{
  "name": "brisbane-city-council-free-native-plants",
  "description": "Council’s Free Native Plants program offers a range of plants to assist you to plant and green your property. The native species provided through the program help grow our city’s urban forest and support local wildlife.",
  "version": "1.7.43",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build --modern",
    "build:analyze": "npm run build -- --analyze",
    "bundlewatch": "bundlewatch --config bundlewatch.config.json",
    "start": "nuxt start",
    "serve:lhci": "npm run start",
    "lhci:autorun": "lhci autorun",
    "generate": "nuxt generate --modern",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:style -- --fix",
    "images": "node images.js",
    "test": "jest",
    "test:e2e": "codeceptjs run --steps"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "1.14.0",
    "@nuxt/typescript-runtime": "2.1.0",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/markdownit": "2.0.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/sitemap": "2.4.0",
    "dayjs": "1.10.5",
    "github-markdown-css": "4.0.0",
    "lodash-es": "4.17.21",
    "mapbox-gl": "2.3.1",
    "nuxt": "2.15.6",
    "store": "2.0.12",
    "vue-warehouse": "2.2.0"
  },
  "devDependencies": {
    "@lhci/cli": "0.6.1",
    "@mdi/js": "5.9.55",
    "@nuxt/image": "0.4.14",
    "@nuxt/types": "2.15.6",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "1.12.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@vue/test-utils": "1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "bundlewatch": "0.3.2",
    "codeceptjs": "3.0.7",
    "codeceptjs-resemblehelper": "1.9.3",
    "cross-env": "7.0.3",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "imagemin-pngquant": "9.0.2",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "playwright": "1.12.2",
    "prettier": "2.2.1",
    "puppeteer": "10.0.0",
    "serve-handler": "6.1.3",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "ts-jest": "26.5.6",
    "ts-node": "10.0.0",
    "vue-jest": "3.0.7"
  }
}
